<div class="page-header jumbotron row">
	<a href="/"><img src="../../../images/logoB.jpg" height="100" width="100px" class="col-md-2"></a>
	<h1 class="col-md-10">Some Random Website</h1>
	<div>
		<tr>
			<td><%= link_to 'Products', products_path %></td>
			<td><%= link_to 'About', pages_about_path %></td>
			<td><%= link_to 'Contact', pages_contact_path %></td>
		</tr>
		<tr>
			<% if user_signed_in? %>
			  <li>You are logged in as <%= current_user.email %></li>
			  <li><%= link_to "Edit", edit_user_path(current_user), method: :update %></li>  
			  <li><%= link_to 'Log out', destroy_user_session_path, method: :delete %></li>
			<% else %>
			  <li><%= link_to "Sign Up", new_user_registration_path %></li>
			  <li><%= link_to "Sign In", new_user_session_path %></li>
			<% end %>
		</tr>
	</div>
</div>